{
  "manifest_version": 3,
  "name": "Image Downloader 2.0",
  "version": "1.0",
  "description": "Download images and copy title to clipboard",
  "permissions": ["activeTab", "scripting", "clipboardWrite"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "etss.png",
      "48": "etss.png",
      "128": "etss.png"
    }
  }
}
